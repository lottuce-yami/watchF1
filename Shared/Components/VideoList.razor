@using F1Project.Data.Database.Types

@foreach(var video in List.Where(video => !string.IsNullOrWhiteSpace(video.Id)))
{
    <a href="watch?v=@video.Id">
        <img src="@video.Preview" alt=""><br>
        <h2>@video.Title</h2>
        <br>
    </a>
}

@code {
    [Parameter]
    public IEnumerable<Video> List { get; set; } = null!;
}